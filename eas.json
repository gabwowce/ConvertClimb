{
    "cli": { "version": ">= 16.20.0", "appVersionSource": "remote" },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal",
            "android": { "buildType": "apk" },
            "ios": { "simulator": true }
        },
        "production": {
            "autoIncrement": true,
            "android": { "buildType": "app-bundle" },
            "ios": { "distribution": "store" }
        }
    },
    "submit": { "production": {} }
}