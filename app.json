{
    "expo": {
        "name": "Gradiator",
        "slug": "Gradiator",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/app-logo.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "splash": {
            "backgroundColor": "#ffffff",
            "resizeMode": "contain"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.gabwowce.gradiator",
            "infoPlist": {
                "ITSAppUsesNonExemptEncryption": false
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/blank.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "package": "com.gabwowce.gradiator"
        },
        "web": {
            "favicon": "./assets/app-logo.png"
        },
        "plugins": [
            [
                "expo-font",
                {
                    "fonts": [
                        "./assets/fonts/coolvetica_rg.ttf"
                    ]
                }
            ],
            [
                "expo-splash-screen",
                {
                    "backgroundColor": "#ffffff",
                    "resizeMode": "contain"
                }
            ]
        ],
        "owner": "gabwowce",
        "extra": {
            "eas": {
                "projectId": "ea70aeec-6e63-4cd7-819a-32fecaa444b4"
            }
        }
    }
}